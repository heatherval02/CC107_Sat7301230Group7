<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/aboutus"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gredient">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/toolbar"
        app:title="About Us"
        app:titleTextColor="@color/cardview_light_background"
        android:background="@drawable/gredient"/>


    <ImageView
        android:id="@+id/image1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="60dp"
        android:src="@drawable/marfil"></ImageView>

    <ImageView
        android:id="@+id/image2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="27dp"
        android:src="@drawable/anjela"></ImageView>

    <ImageView
        android:id="@+id/image3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@id/image1"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="229dp"
        android:src="@drawable/patricia"></ImageView>

    <ImageView
        android:id="@+id/image4"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@id/image2"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="32dp"
        android:src="@drawable/marjorie"></ImageView>

    <ImageView
        android:id="@+id/image5"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@id/image4"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="120dp"
        android:src="@drawable/uy"></ImageView>


    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="7dp"
        android:text="Head Programer"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="224dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:text="Designer and programer"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="76dp"
        android:layout_marginTop="194dp"
        android:text="Database"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/text3.1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="59dp"
        android:layout_marginTop="218dp"
        android:text="Administrator"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/text4"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="200dp"
        android:text="Documentarian"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/text4.1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="261dp"
        android:layout_marginTop="223dp"
        android:text="and Designer"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_below="@+id/image1"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="159dp"
        android:layout_marginTop="430dp"
        android:text="Documentarian"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="93dp" />


</RelativeLayout>